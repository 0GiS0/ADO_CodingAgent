{
  "mcpServers": {
    "context7": {
      "type": "local",
      "command": "npx",
      "tools": [
        "*"
      ],
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "fetch": {
      "type": "local",
      "command": "uvx",
      "tools": [
        "*"
      ],
      "args": [
        "mcp-server-fetch"
      ]
    },
    "time": {
      "type": "local",
      "command": "uvx",
      "tools": [
        "*"
      ],
      "args": [
        "mcp-server-time"
      ]
    }
  }
}