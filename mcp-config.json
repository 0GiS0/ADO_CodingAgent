{
    "mcpServers": {
        "context7": {
            "tools": [
                "*"
            ],
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ]
        },
        "fetch": {
            "type": "stdio",
            "command": "uvx",
            "tools": [
                "*"
            ],
            "args": [
                "mcp-server-fetch"
            ]
        },
        "time": {
            "command": "uvx",
            "args": [
                "mcp-server-time"
            ]
        }
    }
}